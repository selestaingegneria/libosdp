{
    "C_Cpp.files.exclude": {
        "python/vendor/**": true
    },
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/include/",
        "${workspaceFolder}/utils/include/"
    ],
    "files.associations": {
        "**/*.h": "c",
        "*.in": "c",
    },
    "files.watcherExclude": {
        "python/vendor/**": true
    },
    "files.exclude": {
        "python/vendor/**": true,
        "python/build/**": true,
        "**/*.egg-info/**": true
    },
    "python.analysis.extraPaths": [
        "./python"
    ],
    "search.exclude": {
        "python/vendor/**": true
    },
